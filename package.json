{
  "name": "hyper-tree",
  "version": "0.0.1",
  "description": "Appplication to show Tree with node description",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html --public-url ./"
  },
  "keywords": [
    "d3",
    "tree",
    "nodes",
    "hyperapp"
  ],
  "author": "Pavel Belikov",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/env"
    ],

      [
        "@babel/transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "dependencies": {
    "d3": "^5.9.1",
    "hyperapp": "^1.2.9",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "parcel": "^1.11.0",
    "sass": "^1.17.2"
  }
